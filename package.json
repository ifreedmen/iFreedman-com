  "description": "Flagship hub for the iFreedmen Creative Suite: sovereignty, culture, commerce, and communications. Links to .net/.info/.shop/.store. Pages-ready scaffold with shared brand tokens.",
  "main": "create-repos.js",
  "scripts": {
    "help": "node create-repos.js",
    "guide": "node create-repos.js instructions",
    "generate": "node create-repos.js all",
    "html": "node create-repos.js html && echo 'Open repo-creation-helper.html in your browser'",
    "markdown": "node create-repos.js markdown",
    "validate": "node -c create-repos.js && echo 'Scripts are valid'",
    "enhanced": "node enhanced-setup.js enhanced",
    "stable-preview": "node enhanced-setup.js all-enhanced",
    "templates": "node enhanced-setup.js templates",
    "frontend:dev": "cd apps/frontend && npm run dev",
    "frontend:build": "cd apps/frontend && npm run build",
    "frontend:preview": "cd apps/frontend && npm run preview",
    "frontend:install": "cd apps/frontend && npm install"
